        <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="true">
          <stringProp name="BeanShellAssertion.query">String response= new String(ResponseData);
String folder= vars.get(&quot;Folder&quot;);
String fileName = vars.get(&quot;FileName&quot;);
String failCode=&quot;404&quot;;
if (vars.get(&quot;Permission&quot;).equals(&quot;NO&quot;)){
	if (ResponseCode.equals(failCode) == true ) {
		SampleResult.setResponseOK();
		Failure = false;
	}else if (!(response.contains(&quot;document.getElementById(&apos;Username&apos;).focus()&quot;) || response.contains(&quot;&lt;title&gt;Error&lt;/title&gt;&quot;)
			|| response.contains(&quot;&lt;title&gt;Unauthorized&lt;/title&gt;&quot;))){
		FailureMessage = &quot;file &quot;+folder+fileName+&quot; should not be allowed to read directly from this physical address&quot;;
		Failure = true;
		}else{
			SampleResult.setResponseCodeOK();
			Failure = false;
		}
}else{
	if (ResponseCode.equals(failCode) == true ) {
		FailureMessage = &quot;file &quot;+folder+fileName+&quot; should be allowed to read directly from this physical address&quot;;
		Failure = true;
	}else if (response.contains(&quot;document.getElementById(&apos;Username&apos;).focus()&quot;) || response.contains(&quot;&lt;title&gt;Error&lt;/title&gt;&quot;)
		|| response.contains(&quot;&lt;title&gt;Unauthorized&lt;/title&gt;&quot;)){
		FailureMessage = &quot;file &quot;+folder+fileName+&quot; should be allowed to read directly from this physical address&quot;;
		Failure = true;
	}else{
		SampleResult.setResponseCodeOK();
		Failure = false;
	}
}</stringProp>
